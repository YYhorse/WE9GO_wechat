.ServiceDetailLayout{
  display: flex;
  width: 100%;
  flex-direction: column;
  padding: 5px 0;
  border-bottom: 1px solid #ededed;
}
.InputTitleText{
  font-size: 16px;
  color: black;
  font-weight:bold;
  margin-left: 40rpx;
  width: 100%;
}
.InputEditText{
  font-size: 16px;
  color: gray;
  margin-left: 20rpx;
  margin-top: 10rpx;
  width: 100%;
}
.InputContextText{
  display: flex;
  height: 200rpx;
  font-size: 16px;
  color: gray;
  margin-left: 60rpx;
  margin-top: 30rpx;
  margin-bottom: 20rpx;
}
.BottomNewServiceLayout{
  display:flex;
  position: fixed;
  left: 0px;
  bottom: 0px;
  z-index: 50;
  width: 100%;
  height: 48px;
  background-color: #47B118;
  flex-direction:row;
  align-items: center;
  justify-content:center;
}
.NewServiceText{
  font-size: 20px;
  font-weight: bold;
  color: white;
}