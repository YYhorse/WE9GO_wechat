.linecolumngray{
  display: flex;
  flex-direction: row;
  margin-left: 20rpx;
  margin-right: 20rpx;
  border-bottom: 1px solid #ededed;
}
.BuyCarItemLogo{
  width:30rpx;
  height:30rpx;
}
.OrderCodeTitle{
  display: flex;
  width: 100%;
  height: 50rpx;
  flex-direction: row;
  align-items: center;
  background-color: #ededed;
}
.OrderCodeTxt{
   display: flex;
  font-size: 20rpx;
  margin-left: 20rpx;
}

.MenuItemLayout{
  display: flex;
  flex-direction: row;
  align-items: center;
  height: 60rpx;
  padding: 5px 0;
  margin-left: 20rpx;
  margin-right: 20rpx;
  border-bottom: 1px solid #ededed;
}
.MenuItemPhoto{
  width: 40rpx;
  height: 40rpx;
  margin-left: 30rpx;
}
.MenuItemName{
  font-size: 30rpx;
  color: black;
  margin-left: 30rpx;
  width: 350rpx;
  overflow:hidden;      
  text-overflow:ellipsis;
  white-space:nowrap;  
}
.MenuItemNumber{
  width: 250rpx;
  margin-left: 10rpx;
  font-size: 30rpx;
  color:gray;
}
.MenuItemPrice{
  width: 150rpx;
  margin-left: 10rpx;
  font-size: 30rpx;
  color: #FF6101;
}
.MenuTitle{
  display: flex;
  width: 100%;
  height: 50rpx;
  flex-direction: row;
  align-items: center;
  background-color: #ededed;
}

.OneRowTwoDataLayout{
  width: 100%;
  height: 70rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
}
.FirstDataTxt{
  width: 200rpx;
  font-size: 25rpx;
  color: gray;
  margin-left: 30rpx;
}
.SecondDataLayout{
  align-items: center;
  display: flex;
  width: 80%;
  flex-direction: row-reverse;
}
.SecondDataLayout2{
  align-items: center;
  display: flex;
  width: 80%;
  flex-direction: row-reverse;
}
.SecondDataTxt{
  font-size: 28rpx;
  color: gray;
  margin-right: 20rpx;
}